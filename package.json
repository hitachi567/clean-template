{
    "name": "clean-template",
    "version": "0.0.1",
    "description": "clean template for typescript nodejs projects",
    "scripts": {
        "start": "node build/index.js",
        "build": "rimraf build && tsc",
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "test": "ts-mocha -p tsconfig.json src/**/*.test.ts"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/node": "^18.11.2",
        "rimraf": "^3.0.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.8.4"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/hitachi567/clean-template.git"
    },
    "keywords": [
        "template",
        "typescript"
    ],
